<!DOCTYPE html>\par
 <html lang="en">\par
 <head>\par
  <meta charset="UTF-8">\par
  <title>Palindrome or Not</title>\par
 </head>\par
 <body>\par
<script>\par
function palindromeFun (str )\par
\{\par
// convert the string into an array using the string.split() function\par
const arrValue = string.split (''); //\par
\par
// use reverse() method to reverse the array values\par
const reveArrVal = arrValue.reverse();\par
\par
// use join() method to group the array values into the string\par
const revString = reveArrVal.join('');\par
\par
if (string == revString) // if string condition is equal to the revString\par
\{\par
alert('It is a Palindrome '); // print the Palindrome\par
\}\par
else \{\par
alert (' It is not a Palindrome' ); // if the condition is not true.\par
\}\par
\}\par
// take a string from the user\par
const string = prompt( ' Enter the string to check Palindrome ');\par
const value = palindromeFun (string); // call the function\par
console.log(value);\par
</script>\par
<p align="center"><button onclick="history.back()">Back</button></p>\par
</body>\par
